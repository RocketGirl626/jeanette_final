<%= render "layouts/errors", :object => @reference %>

<%= form_for @reference do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :company %>
  <%= f.text_field :company %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.submit %>
<% end %>
