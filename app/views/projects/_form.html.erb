<%= render('layouts/errors', :object => @project) %>

<%= form_for [@category, @project] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :github %>
  <%= f.text_field :github %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <p><%= f.submit %></p>
<% end %>
